
/* ==========================================================================
   01. GENERAL & BASIC STYLES
   ========================================================================== */

#content_wrapper  {
/* 	margin-top: 60px; */
background-color: #f5f5f5;
}
    
/*------------------------------------------*/
/*	 Parallax Settings
/*------------------------------------------*/

.parallax {
	background-attachment: fixed !important;
	background-position: center center;
	background-repeat: no-repeat;
	background-size: cover;
}

#call_to_action { 
	background-image: url(../mylogin.html); 
}

#video { 
	background-image: url(../mylogin.html); 
}

/*------------------------------------------*/
/*	 Headers
/*------------------------------------------*/

h1, h2, h3, h4, h5, h6 {
	margin-top: 0; 
	margin-bottom: 10px;
}

/*------------------------------------------*/
/*	 Lists
/*------------------------------------------*/

ul {
	margin: 0;
	padding: 0;
	list-style: none;
}

/*------------------------------------------*/
/*	 Link Setting
/*------------------------------------------*/

a {
	text-decoration: none;
	-webkit-transition: all 250ms ease-in-out;
	-moz-transition: all 250ms ease-in-out;
	-o-transition: all 250ms ease-in-out;
	-ms-transition: all 250ms ease-in-out;
	transition: all 250ms ease-in-out;
}

a:hover {
	color: #fff;
	text-decoration: none;
}

a:focus {
	color: #f8f8f8;
	outline: none;
	border: none;
	text-decoration: none;
} 

/*------------------------------------------*/
/*	 Theme Button
/*------------------------------------------*/

.btn-theme {
	background-color: #eb801d;
	color: #fff;
	font-size: 15px;
	font-family: 'Raleway', sans-serif;
	text-transform: uppercase;
	font-weight: 600;
	padding: 9px 18px;
	-webkit-border-radius: 0px;
    -moz-border-radius: 0px;
    border-radius: 0px;
	-webkit-transition: all 200ms ease-in-out;
	-moz-transition: all 200ms ease-in-out;
	-o-transition: all 200ms ease-in-out;
	-ms-transition: all 200ms ease-in-out;
	transition: all 200ms ease-in-out;
	margin-top: 10px
}

.btn-theme:hover {
	background-color: #c05e03;
	color: #fff;
}

/*------------------------------------------*/
/*	 Button Hover/Focus
/*------------------------------------------*/

.btn:focus {
	color: #f8f8f8;
	text-decoration: none;
	outline: none;
} 

/*------------------------------------------*/
/*	 Section Titlebar Settings
/*------------------------------------------*/

.titlebar {
	margin-bottom: 40px;
	text-align: center;
}

.titlebar h2 {
	color: #eb801d;
	text-transform: uppercase;
	font-weight: 800;
	margin-bottom: 5px;
}

.titlebar p {
	color: #444;
	font-size: 18px;
	font-family: 'Raleway', sans-serif;
	line-height: 24px;
	font-weight: 500;
	text-transform: uppercase;
	padding: 0 5%;
} 



